{
  "name": "@pymedphys/app",
  "productName": "PyMedPhys",
  "homepage": "https://pymedphys.com",
  "author": "Simon Biggs <me@simonbiggs.net>",
  "version": "0.0.0",
  "description": "PyMedPhys App",
  "scripts": {
    "dist": "electron-webpack && electron-builder build --win --x64"
  },
  "build": {
    "productName": "PyMedPhys",
    "appId": "org.pymedphys.PyMedPhys",
    "files": [
      "build/",
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "devDependencies": {
    "electron": "^7.1.4",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  }
}
